        nco:AgentComponent or nco:Claim
    
    Range: 
        nco:Agent
    
    
ObjectProperty: nco:hasAgentComponent

    Annotations: 
        rdfs:label "has agent component"@en
    
    SubPropertyOf: 
        owl:topObjectProperty
    
    Domain: 
