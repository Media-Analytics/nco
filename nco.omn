    Annotations: 
        rdfs:comment "Captures hierarchical relations between events - e.g., the event \"verdict\" in a trial is a subEventOf of a broader \"trial\" event"@en,
        rdfs:label "is subevent of"@en
    
    SubPropertyOf: 
        nco:eventDependsOn
    
    Domain: 
        nco:Event
    
    Range: 
        nco:Event
    
    InverseOf: 
        nco:hasSubEvent
    
    
