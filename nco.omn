        rdfs:label "event depends on"@en,
        skos:note "## TODO What is the inverse of eventsDependsOn?",
        skos:note "## TODO: range is events OR situation"
    
    SubPropertyOf: 
        nco:dependsOn
    
    Domain: 
        nco:Event
    
    Range: 
        nco:Event or nco:Situation
    
    
ObjectProperty: nco:hasAgent

    Annotations: 
        rdfs:label "has agent"@en
    
