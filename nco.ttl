###  http://data.open.ac.uk/ontology/news-classification#characterisedBy
nco:characterisedBy rdf:type owl:ObjectProperty ;
                    owl:inverseOf nco:characterises .


###  http://data.open.ac.uk/ontology/news-classification#characterises
nco:characterises rdf:type owl:ObjectProperty ;
                  rdfs:domain nco:Description ;
                  rdfs:range nco:Situation .


###  http://data.open.ac.uk/ontology/news-classification#concernsTopic
nco:concernsTopic rdf:type owl:ObjectProperty ;
                  rdfs:domain nco:Claim ;
                  rdfs:range nco:Topic .


###  http://data.open.ac.uk/ontology/news-classification#dependsOn
nco:dependsOn rdf:type owl:ObjectProperty ;
              owl:inverseOf nco:preconditionFor ;
              rdf:type owl:TransitiveProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( nco:Event
                                          nco:Situation
                                        )
                          ] ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( nco:Event
                                         nco:Situation
                                       )
                         ] .


###  http://data.open.ac.uk/ontology/news-classification#eventDependsOn
nco:eventDependsOn rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf nco:dependsOn ;
                   rdfs:domain nco:Event ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( nco:Event
                                              nco:Situation
                                            )
                              ] ;
                   skos:note "## TODO What is the inverse of eventsDependsOn?" ,
                             "## TODO: range is events OR situation" .


###  http://data.open.ac.uk/ontology/news-classification#hasActor
nco:hasActor rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( nco:AgentComponent
                                         nco:Claim
                                       )
                         ] ;
             rdfs:range nco:Agent .


###  http://data.open.ac.uk/ontology/news-classification#hasActorComponent
nco:hasActorComponent rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain nco:Event ;
                      rdfs:range nco:AgentComponent .


###  http://data.open.ac.uk/ontology/news-classification#hasActorRole
nco:hasActorRole rdf:type owl:ObjectProperty ;
                 rdfs:domain nco:AgentComponent ;
                 rdfs:range nco:Agent .


###  http://data.open.ac.uk/ontology/news-classification#hasAspect
nco:hasAspect rdf:type owl:ObjectProperty ;
              owl:inverseOf nco:isAspectOf ;
              rdfs:domain owl:Thing ;
              rdfs:range nco:Topic ;
              skos:note "Topics can have many aspects"@en ,
                        "[Question] check domain"@en .


###  http://data.open.ac.uk/ontology/news-classification#hasClaim
nco:hasClaim rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf nco:hasElement ;
             owl:inverseOf nco:isClaimOf ;
             rdfs:domain nco:Viewpoint ;
             rdfs:range nco:Claim .


###  http://data.open.ac.uk/ontology/news-classification#hasElement
nco:hasElement rdf:type owl:ObjectProperty ;
               owl:inverseOf nco:isElementOf ;
               rdfs:domain nco:Collection ;
               rdfs:range owl:Thing .


###  http://data.open.ac.uk/ontology/news-classification#hasJustification
nco:hasJustification rdf:type owl:ObjectProperty ;
                     skos:note "[Question] K is this the ontology or the example in 4.5.1???" .


###  http://data.open.ac.uk/ontology/news-classification#hasLocation
nco:hasLocation rdf:type owl:ObjectProperty ;
                rdfs:domain nco:Event ;
                rdfs:range nco:Location .


###  http://data.open.ac.uk/ontology/news-classification#hasOmittedEvent
nco:hasOmittedEvent rdf:type owl:ObjectProperty ;
                    rdfs:domain nco:OmissionEvent ;
                    rdfs:range nco:Event .


###  http://data.open.ac.uk/ontology/news-classification#hasPrediction
nco:hasPrediction rdf:type owl:ObjectProperty ;
                  rdfs:domain nco:Event ;
                  rdfs:range owl:Thing ,
                             [ rdf:type owl:Class ;
                               owl:unionOf ( nco:Event
                                             nco:Situation
                                           )
                             ] ;
                  skos:note "[DONE] range is events or situations" ,
                            "[Question] Why situations cannot predict other situations ??? what distinguishes events from situations???" .


###  http://data.open.ac.uk/ontology/news-classification#hasSubEvent
nco:hasSubEvent rdf:type owl:ObjectProperty ;
                owl:inverseOf nco:isSubEventOf ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain nco:Event ;
                rdfs:range nco:Event .


###  http://data.open.ac.uk/ontology/news-classification#hasSubTopic
nco:hasSubTopic rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf nco:isSubTopicOf ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain nco:Topic ;
                rdfs:range nco:Topic ;
                owl:propertyChainAxiom ( nco:hasSubTopic
                                         skos:narrowerTransitive
                                       ) .


###  http://data.open.ac.uk/ontology/news-classification#hasTime
nco:hasTime rdf:type owl:ObjectProperty ;
            rdfs:domain nco:Event ;
            rdfs:range <http://www.w3.org/2006/time#TemporalEntity> .


###  http://data.open.ac.uk/ontology/news-classification#hasTopic
nco:hasTopic rdf:type owl:ObjectProperty ;
             rdfs:domain nco:NewsItem ;
             rdfs:range nco:Topic ;
             owl:propertyChainAxiom ( nco:hasTopic
                                      nco:hasSubTopic
                                    ) ,
                                    ( nco:hasTopic
                                      nco:isTopicRoleOf
                                      nco:isClaimOf
                                      nco:hasTopicRole
                                    ) ;
             rdfs:comment """[DONE] hasTopic (?ni T(?c)) & type (?c Claim) & elementOf (?c ?v) ->  hasTopic (?ni T(?v))

We add a subproperty of elementOf (isClaimOf) and the following chain

hasTopic: hasTopic o isTopicRoleOf o isClaimOf o hasTopicRole (the viewpoint)""" ;
             skos:note "A news item can have many topics, and topics can be associated to many news items."@en ,
                       """[DONE] hasTopic(n, t1) -> All skos:narrowerTransitive (t1,t2) -> subTopicOf(t1, t2)

Two property chains:

hasTopic: hasTopic o hasSubTopic
hasSubTopic: hasSubTopic o skos:narrowerTransitive""" .


###  http://data.open.ac.uk/ontology/news-classification#hasTopicRole
nco:hasTopicRole rdf:type owl:ObjectProperty ;
                 owl:inverseOf nco:isTopicRoleOf ;
                 rdf:type owl:FunctionalProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain owl:Thing ;
                 rdfs:range nco:Topic .


###  http://data.open.ac.uk/ontology/news-classification#hasUnifyingFactor
nco:hasUnifyingFactor rdf:type owl:ObjectProperty ;
                      owl:inverseOf nco:isUnifyingFactorOf ;
                      rdfs:domain nco:Collection ;
                      rdfs:range nco:UnifyingFactor .


###  http://data.open.ac.uk/ontology/news-classification#isAspectOf
nco:isAspectOf rdf:type owl:ObjectProperty .


###  http://data.open.ac.uk/ontology/news-classification#isClaimOf
nco:isClaimOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf nco:isElementOf .


###  http://data.open.ac.uk/ontology/news-classification#isElementOf
nco:isElementOf rdf:type owl:ObjectProperty .


###  http://data.open.ac.uk/ontology/news-classification#isSubEventOf
nco:isSubEventOf rdf:type owl:ObjectProperty .


###  http://data.open.ac.uk/ontology/news-classification#isSubTopicOf
nco:isSubTopicOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:TransitiveProperty ;
                 owl:propertyChainAxiom ( nco:isSubTopicOf
                                          skos:broaderTransitive
                                        ) ;
                 skos:note "[DONE] Align with IPCT as follows: Iptc:topic (?x) and iptc:topic ?y) and Narrower (?x ?y)—> subTopicOf (?x ?y)" .


###  http://data.open.ac.uk/ontology/news-classification#isTopicRoleOf
nco:isTopicRoleOf rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ,
                           owl:InverseFunctionalProperty ;
                  rdfs:domain nco:Topic ;
                  rdfs:range owl:Thing ;
                  skos:note "A given topic role is played by one and only thing."@en .


###  http://data.open.ac.uk/ontology/news-classification#isUnifyingFactorOf
nco:isUnifyingFactorOf rdf:type owl:ObjectProperty .


###  http://data.open.ac.uk/ontology/news-classification#newsItem
nco:newsItem rdf:type owl:ObjectProperty ;
             rdfs:domain owl:Thing ;
             rdfs:range nco:NewsItem ;
             skos:note "### TODO check this one?" .


###  http://data.open.ac.uk/ontology/news-classification#preconditionFor
nco:preconditionFor rdf:type owl:ObjectProperty ,
                             owl:TransitiveProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( nco:Event
                                                nco:Situation
                                              )
                                ] ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( nco:Event
                                               nco:Situation
                                             )
                               ] .


###  http://data.open.ac.uk/ontology/news-classification#preconditionForEvent
nco:preconditionForEvent rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf nco:preconditionFor ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( nco:Event
                                                     nco:Situation
                                                   )
                                     ] ;
                         rdfs:range nco:Event .


###  http://data.open.ac.uk/ontology/news-classification#subTopicOf
nco:subTopicOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf skos:narrowerTransitive ;
               owl:inverseOf nco:superTopicOf ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain nco:Topic .


###  http://data.open.ac.uk/ontology/news-classification#superTopicOf
nco:superTopicOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf skos:broaderTransitive .


