<?xml version="1.0"?>
<rdf:RDF xmlns="http://data.open.ac.uk/ontology/newsclassification/v2#"
     xml:base="http://data.open.ac.uk/ontology/newsclassification/v2"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2">
        <owl:versionIRI rdf:resource="http://github.com/EnricoMotta/ontologies/newsclassification/v2.2.0"/>
        <owl:imports rdf:resource="http://www.w3.org/2000/01/rdf-schema#"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <owl:imports rdf:resource="http://www.w3.org/2006/time#2016"/>
        <terms:title xml:lang="en">News Classification Ontology (NCO)</terms:title>
        <rdfs:comment xml:lang="en">The News Classification Ontology (NCO) v2.2. It defines the concepts needed to discuss news coverage.  These concepts include Entities, Events, Situations, Categorical Topics, Claims and Viewpoints. This ontology extends nco v1.4.3 by adding characterizations of agents (persons and organizations) and further developing the modelling of claims and utterances

Preferred prefix is `nco: &lt;https://data.open.ac.uk/ontology/newsclassification/v2#&gt;`.</rdfs:comment>
        <rdfs:comment xml:lang="en">We import SKOS, TimeOntology</rdfs:comment>
        <rdfs:label xml:lang="en">News Classification Ontology (NCO)</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/contributor -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/contributor"/>
    


    <!-- http://purl.org/dc/terms/created -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/isVersionOf -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/isVersionOf"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/modified -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/modified"/>
    


    <!-- http://purl.org/dc/terms/rights -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/rights"/>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->


    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#pattern -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2001/XMLSchema#pattern"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#duration -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#duration"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gDay -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gDay"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gMonth -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gMonth"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gYear -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gYearMonth -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYearMonth"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#characterisedBy -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#characterisedBy">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#characterises"/>
        <rdfs:label xml:lang="en">characterised by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#characterises -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#characterises">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Description"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Situation"/>
        <rdfs:comment xml:lang="en">This is the main relation linking descriptions to situations. A description provides an interpretation of a situation.</rdfs:comment>
        <rdfs:label xml:lang="en">characterises</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#claimInNewsItem -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#claimInNewsItem">
        <rdfs:subPropertyOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#inNewsItem"/>
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#hasClaim"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Claim"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#NewsItem"/>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#concernsTopic -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#concernsTopic">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Claim"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Topic"/>
        <rdfs:comment>A claim is something that concerns a topic.  The property concernsTopic is used to express this relation</rdfs:comment>
        <rdfs:label xml:lang="en">concerns topic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#dependsOn -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#dependsOn">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#preconditionFor"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#EventOrSituation"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#EventOrSituation"/>
        <rdfs:label xml:lang="en">depends on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#eventDependsOn -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#eventDependsOn">
        <rdfs:subPropertyOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#dependsOn"/>
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#preconditionForEvent"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Event"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#EventOrSituation"/>
        <rdfs:comment xml:lang="en">range is {Event OR Situation}</rdfs:comment>
        <rdfs:label xml:lang="en">event depends on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasAgent -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasAgent">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#AgentComponent"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Agent"/>
        <rdfs:label xml:lang="en">has agent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasAgentComponent -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasAgentComponent">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#Claim"/>
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#AgentComponent"/>
        <rdfs:label xml:lang="en">has agent component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasAgentRole -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasAgentRole">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#AgentComponent"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#AgentRole"/>
        <rdfs:label xml:lang="en">has agent role</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasAspect -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasAspect">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#isAspectOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Entity"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Aspect"/>
        <rdfs:comment xml:lang="en">Connects an entity to its aspects - e.g., the financial status of an individual</rdfs:comment>
        <rdfs:label xml:lang="en">has aspect</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasClaim -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasClaim">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#NewsItem"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Claim"/>
        <rdfs:comment xml:lang="en">Captures the relation between a news item and a claim expressed by an agent, which is reported in the news item in question</rdfs:comment>
        <rdfs:label xml:lang="en">has claim</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasElement -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasElement">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#isElementOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Collection"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasUnifyingFactor"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#ufSatisfiedBy"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">has element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasJustification -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasJustification">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#isJustificationOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Claim"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Justification"/>
        <rdfs:label xml:lang="en">has justification</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasLocation -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasLocation">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Event"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Location"/>
        <rdfs:label xml:lang="en">has location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasNewsSource -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasNewsSource">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#NewsItem"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#NewsSource"/>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasOmittedEvent -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasOmittedEvent">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#isOmittedEventOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#OmissionEvent"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Event"/>
        <rdfs:comment xml:lang="en">captures the relation between an OmissionEvent and the event that has not been carried out - e.g., a soldier&apos;s failure to carry out an order</rdfs:comment>
        <rdfs:label xml:lang="en">has omitted event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasPrediction -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasPrediction">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#PredictionEvent"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#EventOrSituation"/>
        <rdfs:comment xml:lang="en">captures the relation between a PredictionEvent and the event that has been predicted - forecasting an eruption</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasPublisher -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasPublisher">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#NewsSource"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Publisher"/>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasSubEvent -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasSubEvent">
        <rdfs:subPropertyOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#preconditionForEvent"/>
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#subEventOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Event"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Event"/>
        <rdfs:label xml:lang="en">has subevent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasSubTopic -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasSubTopic">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#narrowerTransitive"/>
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#subTopicOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Topic"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Topic"/>
        <rdfs:comment xml:lang="en">This property captures hierarchical relations between topics – e.g., as expressed in the IPTC NewsCodes taxonomy</rdfs:comment>
        <rdfs:label xml:lang="en">has subtopic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasTime -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasTime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2006/time#hasTime"/>
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#isTimeOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#EventOrSituation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#isOmittedEventOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasTime"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">has time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasTopic -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasTopic">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#topicInNewsItem"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#NewsItem"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Topic"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasTopic"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#subTopicOf"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasTopic"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRoleOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#isAspectOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRole"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasTopic"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRoleOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#isClaimOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRole"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasTopic"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRoleOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#object"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRole"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasTopic"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRoleOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#predicate"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRole"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasTopic"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRoleOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#subject"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#topicRole"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasUnifyingFactor -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasUnifyingFactor">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#isUnifyingFactorOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Collection"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#UnifyingFactor"/>
        <rdfs:comment xml:lang="en">Relates a collection to the associated unifying factor</rdfs:comment>
        <rdfs:label xml:lang="en">has unifying factor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasUtterance -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasUtterance">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Claim"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Utterance"/>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasUtteranceType -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasUtteranceType"/>
    


    <!-- http://data.open.ac.uk/ontology/newsclassification/v2#hasViewpoint -->


    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasViewpoint">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#viewpointInNewsItem"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#NewsItem"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/newsclassification/v2#Viewpoint"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#hasClaim"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/newsclassification/v2#isClaimOf"/>
