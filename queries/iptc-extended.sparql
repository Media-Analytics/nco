PREFIX nco: <http://data.open.ac.uk/ontology/news-classification#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX owl: <>


CONSTRUCT {
    <http://data.open.ac.uk/ontology/news-classification/iptc-extended#> a owl:Ontology ;
        owl:imports <https://cv.iptc.org/newscodes/mediatopic?lang=en-US&format=rdfttl> ;
        skos:note """Generated by the SPARQL Anything CLI: java -jar sparql-anything-0.8.2.jar -l cptall-en-GB.ttl -q ../queries/iptc-extended.sparql -o iptc-extended.ttl""".
    ?IPTC a nco:Topic .
} WHERE {
    ?IPTC a skos:Concept 
}
