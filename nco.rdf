<?xml version="1.0"?>
<rdf:RDF xmlns="http://data.open.ac.uk/ontology/news-classification#"
     xml:base="http://data.open.ac.uk/ontology/news-classification"
     xmlns:dul="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#"
     xmlns:nco="http://data.open.ac.uk/ontology/news-classification#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:iptc="http://cv.iptc.org/newscodes/mediatopic/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
