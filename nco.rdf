     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://data.open.ac.uk/ontology/news-classification">
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <owl:imports rdf:resource="http://www.w3.org/2006/time#2016"/>
        <terms:title xml:lang="en">News Classification Ontology (NCO)</terms:title>
        <rdfs:comment xml:lang="en">The News Classification Ontology (NCO). 

Preferred prefix is `nco: &lt;http://data.open.ac.uk/ontology/news-classification#&gt;`.</rdfs:comment>
        <rdfs:comment>[ERROR] There is an error message while runing Hermit, related to isElementOf</rdfs:comment>
        <rdfs:label xml:lang="en">News Classification Ontology (NCO)</rdfs:label>
        <skos:note xml:lang="en">[DONE] We align to DUL using SKOS</skos:note>
        <skos:note xml:lang="en">[DONE] We import IPTC and add axioms to use them as nco:Topic - IPTC (we do it in nco-all)</skos:note>
        <skos:note xml:lang="en">[DONE] We import SKOS, TimeOntology</skos:note>
        <skos:note xml:lang="en">[DONE] specialise owl annotation properties for statements</skos:note>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.open.ac.uk/ontology/news-classification#characterisedBy -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#characterisedBy">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#characterises"/>
        <rdfs:label xml:lang="en">characterised by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#characterises -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#characterises">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Description"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Situation"/>
        <rdfs:label xml:lang="en">characterises</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#concernsTopic -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#concernsTopic">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Claim"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Topic"/>
        <rdfs:label xml:lang="en">concerns topic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#dependsOn -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#dependsOn">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#preconditionFor"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#Event"/>
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#Event"/>
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">depends on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#eventDependsOn -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#eventDependsOn">
        <rdfs:subPropertyOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#dependsOn"/>
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#preconditionForEvent"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Event"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#Event"/>
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">event depends on</rdfs:label>
        <skos:note xml:lang="en">[DONE] range is events OR situation</skos:note>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasAgent -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasAgent">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#AgentComponent"/>
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#Claim"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Agent"/>
        <rdfs:label xml:lang="en">has agent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasAgentComponent -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasAgentComponent">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Event"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#AgentComponent"/>
        <rdfs:label xml:lang="en">has agent component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasAgentRole -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasAgentRole">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#AgentComponent"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Agent"/>
        <rdfs:label xml:lang="en">has actor role</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasAspect -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasAspect">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#isAspectOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label xml:lang="en">has aspect</rdfs:label>
        <skos:note xml:lang="en">entities can be aspects of one another</skos:note>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasClaim -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasClaim">
        <rdfs:subPropertyOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#hasElement"/>
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#isClaimOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Viewpoint"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Claim"/>
        <rdfs:label xml:lang="en">has claim</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasElement -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasElement">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#isElementOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Collection"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label xml:lang="en">has element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasJustification -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasJustification">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#isJustificationOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Claim"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Justification"/>
        <rdfs:label xml:lang="en">has justification</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasLocation -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasLocation">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Event"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Location"/>
        <rdfs:label xml:lang="en">has location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasOmittedEvent -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasOmittedEvent">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#OmissionEvent"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Event"/>
        <rdfs:label xml:lang="en">has omitted event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasPredictedEvent -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasPredictedEvent">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#PredictionEvent"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#Event"/>
                    <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">has prediction</rdfs:label>
        <skos:note>[DONE] range is events or situations</skos:note>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasSubEvent -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasSubEvent">
        <rdfs:subPropertyOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#preconditionForEvent"/>
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#isSubEventOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Event"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Event"/>
        <rdfs:label xml:lang="en">has subevent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasSubTopic -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasSubTopic">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#isSubTopicOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Topic"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Topic"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasSubTopic"/>
            <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#narrowerTransitive"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">has subtopic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasTime -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTime">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:label xml:lang="en">has time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasTopic -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopic">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#newsItem"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#NewsItem"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Topic"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopic"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#isAspectOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopicRole"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopic"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#isTopicRoleOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#isAspectOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopicRole"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopic"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#isTopicRoleOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#isClaimOf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopicRole"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopic"/>
            <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#annotatedSource"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopicRole"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopic"/>
            <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#annotatedTarget"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopicRole"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">has topic</rdfs:label>
        <skos:note xml:lang="en">A news item can have many topics, and topics can be associated to many news items.</skos:note>
        <skos:note>[DONE] hasTopic (?ni T(?c)) &amp; type (?c Claim) &amp; elementOf (?c ?v) -&gt;  hasTopic (?ni T(?v))

We add a subproperty of elementOf (isClaimOf) and the following chain

hasTopic: hasTopic o isTopicRoleOf o isClaimOf o hasTopicRole (the viewpoint)</skos:note>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasTopicRole -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasTopicRole">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#isTopicRoleOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Topic"/>
        <rdfs:label xml:lang="en">has topic role</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#hasUnifyingFactor -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#hasUnifyingFactor">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#isUnifyingFactorOf"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Collection"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#UnifyingFactor"/>
        <rdfs:label xml:lang="en">has unifying factor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#inNewsItem -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#inNewsItem">
        <owl:inverseOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#includesClaim"/>
        <rdfs:label xml:lang="en">in news item</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#includesClaim -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#includesClaim">
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#NewsItem"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Claim"/>
        <rdfs:label>includes claim</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#isAspectOf -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#isAspectOf">
        <rdfs:label xml:lang="en">is aspect of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#isClaimOf -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#isClaimOf">
        <rdfs:subPropertyOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#isElementOf"/>
        <rdfs:label xml:lang="en">is claim of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#isElementOf -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#isElementOf">
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Collection"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#satisfiesUf"/>
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#isUnifyingFactorOf"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">is element of</rdfs:label>
        <skos:note>[DONE] type (?coe CollectionOfEvents) &amp; hasUnifyingFactor (?coe ?uf) &amp; satisfiesUF (?e ?uf) -&gt; elementOf (?e ?coe)</skos:note>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#isJustificationOf -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#isJustificationOf"/>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#isSubEventOf -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#isSubEventOf">
        <rdfs:subPropertyOf rdf:resource="http://data.open.ac.uk/ontology/news-classification#eventDependsOn"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Event"/>
        <rdfs:range rdf:resource="http://data.open.ac.uk/ontology/news-classification#Event"/>
        <rdfs:label xml:lang="en">is subevent of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#isSubTopicOf -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#isSubTopicOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://data.open.ac.uk/ontology/news-classification#isSubTopicOf"/>
            <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#broaderTransitive"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">is subtopic of</rdfs:label>
        <skos:note>[DONE] Align with IPCT as follows: Iptc:topic (?x) and iptc:topic ?y) and Narrower (?x ?y)—&gt; subTopicOf (?x ?y)</skos:note>
    </owl:ObjectProperty>
    


    <!-- http://data.open.ac.uk/ontology/news-classification#isTopicRoleOf -->

    <owl:ObjectProperty rdf:about="http://data.open.ac.uk/ontology/news-classification#isTopicRoleOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://data.open.ac.uk/ontology/news-classification#Topic"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label xml:lang="en">is topic role of</rdfs:label>
        <skos:note xml:lang="en">A given topic role is played by one and only thing.</skos:note>
    </owl:ObjectProperty>
    


